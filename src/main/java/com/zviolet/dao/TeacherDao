package com.zviolet.dao;

import com.zviolet.entity.Teacher;

public interface TeacherDao {
    /**
     * 插入一条教师记录
     * @param record
     * @return
     */
    int insert(Teacher record);

    /**
     * 修改选定的教师记录
     * @param record
     * @return
     */
    int updateById(Teacher record);
}
